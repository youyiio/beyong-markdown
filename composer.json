{
    "name": "youyiio/beyong-markdown",
    "description": "PHP markdown parse, base on parsedown.",
    "keywords": [
        "beyong", "beyongx", "markdown", "parser"
    ],
    "homepage": "https://www.beyongx.com",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "youyiio",
            "email": "developer@jifenbang.net",
            "homepage": "https://www.beyongx.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35"
    },
    "autoload": {
        "psr-0": {"Parsedown": ""},
        "psr-4": {
            "beyong\\": "src/beyong"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "TestParsedown": "test/",
            "ParsedownTest": "test/",
            "CommonMarkTest": "test/",
            "CommonMarkTestWeak": "test/"
        }
    }
}
